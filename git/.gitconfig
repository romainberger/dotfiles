[user]
	name = romainberger
	email = romain@romainberger.com
[github]
	user = romainberger
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
	rmall = !sh -c 'git rm $(git ls-files -d)' -
	count = !git shortlog -sn
[core]
	quotepath = false
	excludesfile = /Users/romainberger/.gitignore_global
[color]
	ui = auto
[color "branch"]
	current = magenta
	local = yellow
	remote = green
[color "diff"]
	meta = yellow
	frag = magenta
	old = red
	new = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
